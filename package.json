{
	"name": "portfolio",
	"version": "0.1.0",
	"description": "joe mackle's personal website using 11ty.js",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=portfolio",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=portfolio --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/joemackle/portfolio.git"
	},
	"author": {
		"name": "Joseph Mackle",
		"email": "jmackle@ufl.edu",
		"url": "https://joemackle.github.io/portfolio/"
	},
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/joemackle/portfolio/issues"
	},
	"homepage": "https://github.com/joemackle/portfolio#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"cross-env": "^7.0.3",
		"luxon": "^3.5.0",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.1"
	}
}
